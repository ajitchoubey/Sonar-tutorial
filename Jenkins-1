node {
  stage ('Clean workspace') {
    cleanWs()
}
  stage ('Build + SonarQube analysis'){
        def MSBUILD_SQ_SCANNER_HOME = tool 'SonarScanner_For_MSBuild'
        
        withSonarQubeEnv('SonarServer') {
          bat "${MSBUILD_SQ_SCANNER_HOME}\\SonarScanner.MSBuild.exe begin /k:FrameworkTest1"
          bat 'MSBuild.exe "C:/JFL Project/SonarPOC/Sonar-tutorial/ConsoleApp4.sln"'
          bat "${MSBUILD_SQ_SCANNER_HOME}\\SonarScanner.MSBuild.exe end"
        }
     }
}


