node {
  stage ('Clean workspace') {
    cleanWs()
}
  stage ('Build + SonarQube analysis'){
        def MSBUILD_SQ_SCANNER_HOME = tool 'SonarScanner_For_MSBuild'
        
        withSonarQubeEnv('SonarServer') {
          bat "dotnet ${MSBUILD_SQ_SCANNER_HOME}\\SonarScanner.MSBuild.dll begin /k:FrameworkTest1"
          bat 'dotnet build "C:/JFL Project/Framework-Sonar/Sonar-tutorial/WebApplication1.sln"'
          bat "dotnet ${MSBUILD_SQ_SCANNER_HOME}\\SonarScanner.MSBuild.dll end"
        }
     }
}


