node {
  stage ('Clean workspace') {
    cleanWs()
}
  stage ('Build + SonarQube analysis'){
        def MSBUILD_SQ_SCANNER_HOME = tool 'SonarScanner_For_MSBuild'
        
        withSonarQubeEnv('SonarServer') {
          bat "dotnet ${MSBUILD_SQ_SCANNER_HOME}\\SonarScanner.MSBuild.dll begin /k:POSDemo"
          bat 'dotnet build "C:/JFL Project/POSDemo/Sonar-tutorial/JublFood.POS.App.sln"'
          bat "dotnet ${MSBUILD_SQ_SCANNER_HOME}\\SonarScanner.MSBuild.dll end"
        }
     }
}


